{
  "manifest_version": 3,
  "name": "BlockSiteExtension",
  "version": "1.0",
  "permissions": [
    "tabs",
    "storage",
    "declarativeNetRequest",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
